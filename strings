#include <stdio.h>
#include <string.h>
int main() {
def print_greeting():
    print("Hello, World!")
def print_personalized_greeting(name):
    print(f"Hello, {name}!")

def print_farewell():
    print("Goodbye!")

def print_motivation():
    print("Keep pushing forward!")

print_greeting()
print_personalized_greeting("Alice")
print_farewell()
print_motivation()
    }
