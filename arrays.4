#include <stdio.h>

int main() {
    int arr[] = {12, 35, 87, 23, 56};
    int largest = arr[0];
    int n = sizeof(arr) / sizeof(arr[0]);
    
    for (int i = 1; i < n; i++) {
        if (arr[i] > largest) {
            largest = arr[i];
        }
    }
    printf("The largest element is: %d\n", largest);
    return 0;
}
